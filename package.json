{
  "name": "ffy-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --port $PORT",
    "dev": "vite --port=3000",
    "build": "npx tsc-silent -p './tsconfig.json' --suppress @ && vite build",
    "serve-prod": "vite build && node server.js",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:prod": "./scripts/docker-prod.sh",
    "lint:scripts": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "lint:scripts:fix": "npm run lint:scripts -- --fix",
    "lint:scripts:report": "npm run lint:scripts -- --format html --output-file ./reports/eslint.html",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "format": "prettier --ignore-unknown --find-config-path --check .",
    "format:fix": "prettier --ignore-unknown --find-config-path --write .",
    "lint:ts": "tsc --noEmit",
    "lint": "sh shells/lint",
    "lint:fix": "sh shells/lint-fix",
    "lint:report": "sh shells/lint-report",
    "lint:staged": "lint-staged",
    "preview": "vite preview",
    "validate": "sh ./shells/validate",
    "validate:fix": "sh ./shells/validate-fix",
    "clean": "rimraf node_modules dist reports"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fontsource/roboto": "^4.5.8",
    "@fullcalendar/core": "^6.1.9",
    "@fullcalendar/daygrid": "^6.1.9",
    "@fullcalendar/react": "^6.1.9",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@react-google-maps/api": "^2.19.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@turf/turf": "^6.5.0",
    "@types/express": "^4.17.20",
    "@uiw/react-color-compact": "^2.1.1",
    "autosuggest-highlight": "^3.3.4",
    "chart.js": "^4.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "formik": "^2.4.5",
    "geobuf": "^3.0.2",
    "geojson-to-kml": "^0.0.1",
    "pbf": "^3.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.16.0",
    "react-svg": "^16.1.28",
    "react-swipeable-views-react-18-fix": "^0.14.1",
    "wkt": "^0.1.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-date-range": "^1.4.7",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "glob": "^10.3.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.3.2",
    "vite-plugin-svgr": "^3.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
